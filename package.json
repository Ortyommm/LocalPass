{
  "name": "local-pass",
  "productName": "LocalPass",
  "version": "1.2.2",
  "description": "Храни свои пароли в безопасности!",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon --exec electron .",
    "build-installer": "electron-builder"
  },
  "build": {
    "npmRebuild": false,
    "appId": "local-pass",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "icon.ico"
    },
    "nsis": {
      "installerIcon": "icon.ico",
      "uninstallerIcon": "icon.ico",
      "uninstallDisplayName": "LocalPass",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "keywords": [
    "Electron"
  ],
  "author": "Artem Margaryan",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "cross-env": "^7.0.3",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "browserslist": "^4.16.6",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.5",
    "css-minimizer-webpack-plugin": "^3.0.0",
    "fork-ts-checker-webpack-plugin": "^6.2.12",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^6.0.0",
    "postcss": "^8.3.0",
    "postcss-loader": "^5.3.0",
    "postcss-preset-env": "^6.7.0",
    "sass-loader": "^11.1.1",
    "sharp": "^0.28.3",
    "style-loader": "^2.0.0",
    "thread-loader": "^3.0.4",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.4",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "electron": "^9.0.0",
    "electron-builder": "^22.11.7",
    "electron-packager": "^14.2.1",
    "node-gyp": "^8.2.0",
    "nodemon": "^2.0.3"
  },
  "dependencies": {
    "bootstrap-icons": "^1.5.0",
    "electron-log": "^4.1.2",
    "glob": "^7.2.0",
    "jszip": "^3.7.1",
    "platform-folders": "^0.5.4"
  }
}
